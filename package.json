{
  "name": "regitra",
  "version": "0.0.0",
  "scripts": {
    "build:internal:json": "node ./builder/json.js",
    "build:internal:img": "node ./builder/img.js",
    "build:pre": "npm run build:internal:json && npm run build:internal:img",
    "dev": "npm run build:pre && vite",
    "build": "npm run build:pre && tsc && vite build",
    "build:fast": "tsc && vite build",
    "serve": "npm run build && vite preview",
    "test:unit": "vitest run",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^4.1.1",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "terser": "^5.19.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "draggable_dialog": "^1.0.3"
  }
}
